<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:System="clr-namespace:System;assembly=mscorlib"
        xmlns:local="clr-namespace:NoodleManagerX"
        mc:Ignorable="d" d:DesignWidth="950" d:DesignHeight="622"
        Width="950" Height="622"
        x:Class="NoodleManagerX.MainWindow"
        HasSystemDecorations="False"
        Icon="resm:NoodleManagerX.Assets.Included.icon-nm.ico"
        Title="NoodleManagerX"
		FontFamily="resm:NoodleManagerX.Assets.Included?assembly=NoodleManagerX#Noto Sans">

	<!--<Window.Background>
		<ImageBrush Source="C:\Users\Max\Downloads\Test.jpg" Stretch="Fill" />
	</Window.Background>-->

	<Window.Resources>
		<local:EqualsConverter x:Key="EqualsConverter"/>
		<local:TabHilightConverter x:Key="TabHilightConverter"/>
		<local:DifficultyColorConverter x:Key="DifficultyColorConverter"/>
		<local:BoolPathConverter x:Key="BoolPathConverter"/>
		<local:TwoParameterPathConverter x:Key="TwoParameterPathConverter"/>
		<local:BoolColorConverter x:Key="BoolColorConverter"/>
		<local:TwoParameterColorConverter x:Key="TwoParameterColorConverter"/>
		<local:StringToBoolConverter x:Key="StringToBoolConverter"/>
		<local:NullToBoolConverter x:Key="NullToBoolConverter"/>
		<local:NotConverter x:Key="NotConverter"/>

		<Color x:Key="ThemeBackgroundColor">#FF282828</Color>
		<Color x:Key="ThemeBorderLowColor">#FF505050</Color>
		<Color x:Key="ThemeBorderMidColor">#FF808080</Color>
		<Color x:Key="ThemeBorderHighColor">#FFA0A0A0</Color>
		<Color x:Key="ThemeControlLowColor">#FF282828</Color>
		<Color x:Key="ThemeControlMidColor">#FF505050</Color>
		<Color x:Key="ThemeControlMidHighColor">#FF686868</Color>
		<Color x:Key="ThemeControlHighColor">#FF808080</Color>
		<Color x:Key="ThemeControlVeryHighColor">#FFEFEBEF</Color>
		<Color x:Key="ThemeControlHighlightLowColor">#FFA8A8A8</Color>
		<Color x:Key="ThemeControlHighlightMidColor">#FF828282</Color>
		<Color x:Key="ThemeControlHighlightHighColor">#FF505050</Color>
		<Color x:Key="ThemeForegroundColor">#FFDEDEDE</Color>
		<Color x:Key="HighlightColor">#FF119EDA</Color>

		<SolidColorBrush x:Key="TabBarColor" Color="#c0000000"/>
		<SolidColorBrush x:Key="WindowControlColor" Color="#ffffff"/>
		<SolidColorBrush x:Key="TabActiveColor" Color="#f91c85"/>
		<SolidColorBrush x:Key="TabInactiveColor" Color="#aa49e0"/>
		<SolidColorBrush x:Key="ControlHeaderColor" Color="#c0322680"/>
		<SolidColorBrush x:Key="ControlColor" Color="Red"/>
		<SolidColorBrush x:Key="ConnectedColor" Color="#ffffff"/>
		<SolidColorBrush x:Key="NotConnectedColor" Color="#888888"/>
		<SolidColorBrush x:Key="CaretColor" Color="#ff00ff"/>
		<SolidColorBrush x:Key="BackgroundColor" Color="transparent"/>
		<SolidColorBrush x:Key="SeparatorColor" Color="#999999"/>
		<SolidColorBrush x:Key="DownloadedColor" Color="#888888"/>
		<SolidColorBrush x:Key="NotDownloadedColor" Color="#ffffff"/>
		<SolidColorBrush x:Key="BackgroundDownloadedColor" Color="#80505050"/>
		<SolidColorBrush x:Key="BackgroundNotDownloadedColor" Color="transparent"/>
		<SolidColorBrush x:Key="DifficultyActiveDownloadedColor" Color="#bbbbbb"/>
		<SolidColorBrush x:Key="DifficultyInactiveDownloadedColor" Color="#888888"/>
		<SolidColorBrush x:Key="DifficultyActiveNotDownloadedColor" Color="#ffffff"/>
		<SolidColorBrush x:Key="DifficultyInactiveNotDownloadedColor" Color="#888888"/>
		<SolidColorBrush x:Key="PathValidColor" Color="#aaaaaa"/>
		<SolidColorBrush x:Key="PathInvalidColor" Color="#ff0000"/>
		<SolidColorBrush x:Key="ThemeBackgroundBrush" Color="{StaticResource ThemeBackgroundColor}" />
		<SolidColorBrush x:Key="ThemeBorderLowBrush" Color="{StaticResource ThemeBorderLowColor}" />
		<SolidColorBrush x:Key="ThemeBorderMidBrush" Color="{StaticResource ThemeBorderMidColor}" />
		<SolidColorBrush x:Key="ThemeBorderHighBrush" Color="{StaticResource ThemeBorderHighColor}" />
		<SolidColorBrush x:Key="ThemeControlLowBrush" Color="{StaticResource ThemeControlLowColor}" />
		<SolidColorBrush x:Key="ThemeControlMidBrush" Color="{StaticResource ThemeControlMidColor}" />
		<SolidColorBrush x:Key="ThemeControlMidHighBrush" Color="{StaticResource ThemeControlMidHighColor}" />
		<SolidColorBrush x:Key="ThemeControlHighBrush" Color="{StaticResource ThemeControlHighColor}" />
		<SolidColorBrush x:Key="ThemeControlVeryHighBrush" Color="{StaticResource ThemeControlVeryHighColor}" />
		<SolidColorBrush x:Key="ThemeControlHighlightLowBrush" Color="{StaticResource ThemeControlHighlightLowColor}" />
		<SolidColorBrush x:Key="ThemeControlHighlightMidBrush" Color="{StaticResource ThemeControlHighlightMidColor}" />
		<SolidColorBrush x:Key="ThemeControlHighlightHighBrush" Color="{StaticResource ThemeControlHighlightHighColor}" />
		<SolidColorBrush x:Key="ThemeForegroundBrush" Color="{StaticResource ThemeForegroundColor}" />
		<SolidColorBrush x:Key="HighlightBrush" Color="{StaticResource HighlightColor}" />
		<!--
		<SolidColorBrush x:Key="ControlHeaderColor" Color="#322680"/>
		<SolidColorBrush x:Key="BackgroundColor" Color="#1a1b1f"/>
		<SolidColorBrush x:Key="SeparatorColor" Color="#999999"/>
		<SolidColorBrush x:Key="NotDownloadedColor" Color="#ffffff"/>
		<SolidColorBrush x:Key="DownloadedColor" Color="#888888"/>
		<SolidColorBrush x:Key="TabActiveColor" Color="#f91c85"/>
		<SolidColorBrush x:Key="TabInactiveColor" Color="#aa49e0"/>
		<SolidColorBrush x:Key="DifficultyActiveColor" Color="#ffffff"/>
		<SolidColorBrush x:Key="DifficultyInactiveColor" Color="#888888"/>
		<SolidColorBrush x:Key="CaretColor" Color="#ffffff"/>
		<SolidColorBrush x:Key="PathValidColor" Color="#aaaaaa"/>
		<SolidColorBrush x:Key="PathInvalidColor" Color="#ff0000"/>
		-->
	</Window.Resources>

	<Window.Styles>

		<!--Set font-->
		<Style Selector="TextBlock">
			<Setter Property="Foreground" Value="{StaticResource ControlColor}"></Setter>
		</Style>

		<!--ToolTips-->
		<Style Selector="Button">
			<Setter Property="ToolTip.ShowDelay" Value="1000"/>
		</Style>
		<Style Selector="ComboBox">
			<Setter Property="ToolTip.ShowDelay" Value="1000"/>
		</Style>

		<!--Button without animations-->
		<Style Selector="Button.BlankButton">
			<Setter Property="Background" Value="Transparent"/>
			<Setter Property="BorderThickness" Value="0"/>
			<Setter Property="Padding" Value="0"/>
		</Style>
		<Style Selector="Button.BlankButton:pointerover > ContentPresenter">
			<Setter Property="Background" Value="Transparent"/>
		</Style>

		<!--Combobox without animations-->
		<Style Selector="ComboBox.BlankBox">
			<Setter Property="BorderThickness" Value="0"/>
		</Style>
		<Style Selector="ComboBox.BlankBox ToggleButton ContentPresenter">
			<Setter Property="Background" Value="Transparent"/>
		</Style>


		<!--Style for the tab header-->
		<Style Selector="Button.Tab">
			<Setter Property="VerticalAlignment" Value="Center"/>
			<Setter Property="Margin" Value="10,0,10,0"/>
			<Setter Property="FontWeight" Value="Bold"/>
			<Setter Property="FontSize" Value="20"/>
			<Setter Property="ClickMode" Value="1"/>
		</Style>

		<!--Style for minimize, maximize, close-->
		<Style Selector="Button.WControls">
			<Setter Property="VerticalAlignment" Value="Top"/>
			<Setter Property="Height" Value="20"/>
			<Setter Property="Margin" Value="3,0,0,0"/>
		</Style>

		<!--Style for listbox-->
		<Style Selector="ListBox.MyListBox">
			<Setter Property="SelectionMode" Value="Single"/>
		</Style>
		<Style Selector="ListBox.MyListBox">
			<Setter Property="AutoScrollToSelectedItem" Value="True"/>
		</Style>
		<Style Selector="ListBox.MyListBox">
			<Setter Property="BorderThickness" Value="0"/>
		</Style>
		<Style Selector="ListBox.MyListBox">
			<Setter Property="Background" Value="{StaticResource BackgroundColor}"/>
		</Style>
		<Style Selector="ListBox.MyListBox ScrollViewer">
			<Setter Property="HorizontalScrollBarVisibility" Value="Disabled"/>
		</Style>
		<Style Selector="ListBox.MyListBox RepeatButton">
			<Setter Property="IsVisible" Value="False"/>
		</Style>
		<Style Selector="ListBox.MyListBox ScrollBar:vertical">
			<Setter Property="Width" Value="22"/>
		</Style>
		<Style Selector="ListBox.MyListBox ScrollBar:vertical Thumb">
			<Setter Property="Width" Value="22"/>
		</Style>
		<Style Selector="ListBox.MyListBox ListBoxItem">
			<Setter Property="Padding" Value="0"/>
			<Setter Property="IsSelected" Value="{Binding selected, Mode=TwoWay}"/>
			<Setter Property="FocusAdorner" Value="{x:Null}"/>
			<Setter Property="ContextMenu">
				<Setter.Value>
					<ContextMenu Background="{StaticResource BackgroundColor}" IsVisible="{Binding Path=description, Converter={StaticResource StringToBoolConverter}}">
						<TextBlock Text="{Binding description}" TextWrapping="Wrap" Width="{Binding Source.Width}"/>
					</ContextMenu>
				</Setter.Value>
			</Setter>
		</Style>
		<Style Selector="ListBox.MyListBox ItemsPresenter">
			<Setter Property="Margin" Value="0"/>
		</Style>
		<Style Selector="ListBox.MyListBox ListBoxItem > ContentPresenter">
			<Setter Property="Background" Value="{Binding Path=downloaded, Converter={StaticResource BoolColorConverter}, ConverterParameter=BackgroundDownloadedColor|BackgroundNotDownloadedColor}"/>
		</Style>


		<!--Different List Boxes-->
		<Style Selector="ListBox.MyListBoxSmall ListBoxItem">
			<Setter Property="Height" Value="162"/>
		</Style>
		<Style Selector="ListBox.MyListBoxSmall ListBoxItem:pointerover Image">
			<Setter Property="Width" Value="160"/>
			<Setter Property="Height" Value="160"/>
		</Style>
		<Style Selector="ListBox.MyListBoxSmall ListBoxItem:selected Image">
			<Setter Property="Width" Value="160"/>
			<Setter Property="Height" Value="160"/>
		</Style>
		<Style Selector="ListBox.MyListBoxSmall ListBoxItem:not(:pointerover):not(:selected) Image">
			<Setter Property="Width" Value="140"/>
			<Setter Property="Height" Value="140"/>
		</Style>

		<Style Selector="ListBox.MyListBoxBig ListBoxItem">
			<Setter Property="Height" Value="242"/>
		</Style>
		<Style Selector="ListBox.MyListBoxBig ListBoxItem:pointerover Image">
			<Setter Property="Width" Value="182"/>
			<Setter Property="Height" Value="240"/>
		</Style>
		<Style Selector="ListBox.MyListBoxBig ListBoxItem:selected Image">
			<Setter Property="Width" Value="182"/>
			<Setter Property="Height" Value="240"/>
		</Style>
		<Style Selector="ListBox.MyListBoxBig ListBoxItem:not(:pointerover):not(:selected) Image">
			<Setter Property="Width" Value="167"/>
			<Setter Property="Height" Value="220"/>
		</Style>

		<!--Style for the difficulties-->
		<Style Selector="TextBlock.Difficulty">
			<Setter Property="VerticalAlignment" Value="Center"/>
			<Setter Property="Margin" Value="20,0,0,0"/>
			<Setter Property="FontWeight" Value="Bold"/>
			<Setter Property="FontSize" Value="15"/>
		</Style>

		<!--Change text color when downloaded-->
		<Style Selector="TextBlock.Downloaded">
			<Setter Property="Foreground" Value="{Binding Path=downloaded, Converter={StaticResource BoolColorConverter}, ConverterParameter=DownloadedColor|NotDownloadedColor}"/>
		</Style>

		<!--Make text overlap the next grid-->
		<Style Selector="TextBlock.DynamicText">
			<Setter Property="ClipToBounds" Value="false"/>
			<Setter Property="Width" Value="1"/>
			<Setter Property="HorizontalAlignment" Value="Left"/>
		</Style>

		<!--Some general settings for the SVGs-->
		<Style Selector="Svg">
			<Setter Property="EnableCache" Value="True"/>
			<Setter Property="UseLayoutRounding" Value="True"/>
		</Style>

		<Style Selector="Path">
			<Setter Property="Stretch" Value="Uniform"/>
			<Setter Property="HorizontalAlignment" Value="Center"/>
			<Setter Property="VerticalAlignment" Value="Center"/>
			<Setter Property="UseLayoutRounding" Value="True"/>
		</Style>

		<!--Settings-->
		<Style Selector="Grid.SettingsGrid">
			<Setter Property="Margin" Value="10,10,10,10"/>
		</Style>
		<Style Selector="TextBlock.SettingsLabel">
			<Setter Property="Margin" Value="16,0,0,0"/>
			<Setter Property="VerticalAlignment" Value="Center"/>
		</Style>
		<Style Selector="Button.Socials">
			<Setter Property="Margin" Value="2,0,2,0"/>
		</Style>

		<!--Template for listbox items of maps, playlists, stages and avatars-->
		<Style Selector="ListBox.ContentItem" >
			<Setter Property="ItemTemplate">
				<DataTemplate>
					<!--v content, separator v-->
					<Grid RowDefinitions="*,auto,auto">
						<!--< cover, content >-->
						<Grid ColumnDefinitions="auto,*" Grid.Row="0">
							<Image Grid.Column="0" Source="{Binding cover_bmp}" Stretch="UniformToFill"/>
							<!--< info, stats, buttons >-->
							<Grid ColumnDefinitions="*,auto,auto" Grid.Column="1" Margin="20,0,10,5">
								<!--v title, artist, mapper, *, duration/difficulty v-->
								<Grid RowDefinitions="auto,auto,auto,*,auto">
									<TextBlock Grid.Row="0" Text="{Binding display_title}" Classes="Downloaded DynamicText" FontSize="35"/>
									<TextBlock Grid.Row="1" Text="{Binding artist}" Classes="Downloaded DynamicText" FontSize="20"/>
									<TextBlock Grid.Row="2" Text="{Binding display_creator}" Classes="Downloaded DynamicText" FontSize="20"/>
									<!--< duration, difficulty>-->
									<Grid Grid.Row="4" ColumnDefinitions="auto,*">
										<TextBlock Grid.Column="0" Text="{Binding duration}" Classes="Downloaded DynamicText" FontSize="30" FontWeight="SemiBold" Width="77"/>
										<Grid Grid.Column="1" ColumnDefinitions="*,auto,auto,auto,auto,auto,auto" HorizontalAlignment="Center" IsVisible="{Binding Path=display_difficulties, Converter={StaticResource NullToBoolConverter}}">
											<TextBlock Grid.Column="1" Text="EASY" Classes="Difficulty">
												<TextBlock.Foreground>
													<MultiBinding Converter="{StaticResource DifficultyColorConverter}"
																  ConverterParameter="0">
														<Binding Path="display_difficulties"/>
														<Binding Path="downloaded"/>
													</MultiBinding>
												</TextBlock.Foreground>
											</TextBlock>
											<TextBlock Grid.Column="2" Text="NORMAL" Classes="Difficulty">
												<TextBlock.Foreground>
													<MultiBinding Converter="{StaticResource DifficultyColorConverter}"
																  ConverterParameter="1">
														<Binding Path="display_difficulties"/>
														<Binding Path="downloaded"/>
													</MultiBinding>
												</TextBlock.Foreground>
											</TextBlock>
											<TextBlock Grid.Column="3" Text="HARD" Classes="Difficulty">
												<TextBlock.Foreground>
													<MultiBinding Converter="{StaticResource DifficultyColorConverter}"
																  ConverterParameter="2">
														<Binding Path="display_difficulties"/>
														<Binding Path="downloaded"/>
													</MultiBinding>
												</TextBlock.Foreground>
											</TextBlock>
											<TextBlock Grid.Column="4" Text="EXPERT" Classes="Difficulty">
												<TextBlock.Foreground>
													<MultiBinding Converter="{StaticResource DifficultyColorConverter}"
																  ConverterParameter="3">
														<Binding Path="display_difficulties"/>
														<Binding Path="downloaded"/>
													</MultiBinding>
												</TextBlock.Foreground>
											</TextBlock>
											<TextBlock Grid.Column="5" Text="MASTER" Classes="Difficulty">
												<TextBlock.Foreground>
													<MultiBinding Converter="{StaticResource DifficultyColorConverter}"
																  ConverterParameter="4">
														<Binding Path="display_difficulties"/>
														<Binding Path="downloaded"/>
													</MultiBinding>
												</TextBlock.Foreground>
											</TextBlock>
											<TextBlock Grid.Column="6" Text="CUSTOM" Classes="Difficulty">
												<TextBlock.Foreground>
													<MultiBinding Converter="{StaticResource DifficultyColorConverter}"
																  ConverterParameter="5">
														<Binding Path="display_difficulties"/>
														<Binding Path="downloaded"/>
													</MultiBinding>
												</TextBlock.Foreground>
											</TextBlock>
										</Grid>
									</Grid>
								</Grid>
								<!--v stats, icons v-->
								<Grid Grid.Column="1" RowDefinitions="auto,auto,auto" ColumnDefinitions="*,auto" VerticalAlignment="Center">
									<TextBlock Grid.Column="0" Grid.Row="0" Text="{Binding upvote_count}" Classes="Downloaded DynamicText" FontSize="20" TextAlignment="Right"/>
									<Path Grid.Column="1" Grid.Row="0" Height="10" Fill="{Binding Path=downloaded, Converter={StaticResource BoolColorConverter}, ConverterParameter=DownloadedColor|NotDownloadedColor}">
										<Path.Data>
											<PathGeometry Figures="M 6.6152344 0 A 0.9261326 0.9261326 0 0 0 5.9375 0.29296875 L 0.25 6.3789062 A 0.92603999 0.92603999 0 0 0 0.29296875 7.6875 0.92603999 0.92603999 0 0 0 1.6035156 7.6445312 L 6.6152344 2.2832031 11.626953 7.6445312 A 0.92603999 0.92603999 0 0 0 12.935547 7.6875 0.92603999 0.92603999 0 0 0 12.980469 6.3789062 L 7.2910156 0.29296875 A 0.9261326 0.9261326 0 0 0 6.6152344 0 Z" FillRule="NonZero"/>
										</Path.Data>
									</Path>
									<TextBlock Grid.Column="0" Grid.Row="1" Text="{Binding downvote_count}" Classes="Downloaded DynamicText" FontSize="20" TextAlignment="Right"/>
									<Path Grid.Column="1" Grid.Row="1" Height="10" Fill="{Binding Path=downloaded, Converter={StaticResource BoolColorConverter}, ConverterParameter=DownloadedColor|NotDownloadedColor}">
										<Path.Data>
											<PathGeometry Figures="M 6.6152344 0 A 0.9261326 0.9261326 0 0 0 5.9375 0.29296875 L 0.25 6.3789062 A 0.92603999 0.92603999 0 0 0 0.29296875 7.6875 0.92603999 0.92603999 0 0 0 1.6035156 7.6445312 L 6.6152344 2.2832031 11.626953 7.6445312 A 0.92603999 0.92603999 0 0 0 12.935547 7.6875 0.92603999 0.92603999 0 0 0 12.980469 6.3789062 L 7.2910156 0.29296875 A 0.9261326 0.9261326 0 0 0 6.6152344 0 Z" FillRule="NonZero"/>
										</Path.Data>
										<Path.RenderTransform>
											<RotateTransform Angle="180"/>
										</Path.RenderTransform>
									</Path>
									<TextBlock Grid.Column="0" Grid.Row="2" Text="{Binding download_count}" Classes="Downloaded DynamicText" FontSize="20" TextAlignment="Right"/>
									<Button Grid.Column="1" Grid.Row="2" Classes="BlankButton" Height="30" Margin="10,0,10,0" Command="{Binding downloadCommand}">
										<Path>
											<Path.Fill>
												<MultiBinding Converter="{StaticResource TwoParameterColorConverter}"
															  ConverterParameter="NotDownloadedColor|DownloadedColor|DownloadedColor|DownloadedColor">
													<Binding Path="downloaded"/>
													<Binding Path="downloading"/>
												</MultiBinding>
											</Path.Fill>
											<Path.Data>
												<PathGeometry Figures="M 25.001953 -0.0078125 A 3.5000126 3.5000126 0 0 0 21.503906 3.4921875 L 21.503906 25.03125 L 15.863281 19.414062 A 3.5000126 3.5000126 0 0 0 13.382812 18.396484 A 3.5000126 3.5000126 0 0 0 10.910156 19.421875 A 3.5000126 3.5000126 0 0 0 10.925781 24.375 L 22.529297 35.935547 A 3.5003626 3.5003626 0 0 0 27.46875 35.935547 L 39.080078 24.375 A 3.5000126 3.5000126 0 0 0 39.087891 19.421875 A 3.5000126 3.5000126 0 0 0 34.134766 19.414062 L 28.501953 25.027344 L 28.501953 3.4921875 A 3.5000126 3.5000126 0 0 0 25.001953 -0.0078125 z M 3.5058594 30.634766 A 3.5000126 3.5000126 0 0 0 0.0078125 34.134766 L 0.0078125 46.455078 A 3.5003626 3.5003626 0 0 0 3.5058594 49.960938 L 46.490234 49.960938 A 3.5003626 3.5003626 0 0 0 49.990234 46.455078 L 49.990234 34.134766 A 3.5000126 3.5000126 0 0 0 46.490234 30.634766 A 3.5000126 3.5000126 0 0 0 42.992188 34.134766 L 42.992188 42.955078 L 7.0058594 42.955078 L 7.0058594 34.134766 A 3.5000126 3.5000126 0 0 0 3.5058594 30.634766 z " FillRule="NonZero"/>
											</Path.Data>
										</Path>
										<ToolTip.Tip>
											<MultiBinding Converter="{StaticResource TwoParameterPathConverter}"
															  ConverterParameter="Download|Downloaded|Downloading|Downloaded">
												<Binding Path="downloaded"/>
												<Binding Path="downloading"/>
											</MultiBinding>
										</ToolTip.Tip>
									</Button>
								</Grid>
								<!--v buttons v-->
								<Grid Grid.Column="2" RowDefinitions="auto,auto" VerticalAlignment="Center" Margin="10,0,10,0">
									<Button Grid.Row="0" Classes="BlankButton" Height="20" Width="20" Margin="0,0,0,5" Command="{Binding playPreviewCommand}" IsVisible="{Binding Path=display_preview, Converter={StaticResource StringToBoolConverter}}"
											ToolTip.Tip="{Binding Path=playing, Converter={StaticResource BoolPathConverter}, ConverterParameter=Play|Stop}">
										<Grid>
											<!--play-->
											<Path IsVisible="{Binding Path=playing, Converter={StaticResource NotConverter}}" Fill="{Binding Path=downloaded, Converter={StaticResource BoolColorConverter}, ConverterParameter=DownloadedColor|NotDownloadedColor}">
												<Path.Data>
													<PathGeometry Figures="M 1.3398437 0.09765625 C 0.7242703 -0.20969603 3.4690485e-4 0.23774352 0 0.92578125 V 12.302734 c 3.4690485e-4 0.688038 0.7242703 1.135477 1.3398437 0.828125 L 12.716797 7.4433594 c 0.681983 -0.3414715 0.681983 -1.3147785 0 -1.65625 z" FillRule="NonZero"/>
												</Path.Data>
											</Path>
											<!--stop-->
											<Path IsVisible="{Binding playing}" Fill="{Binding Path=downloaded, Converter={StaticResource BoolColorConverter}, ConverterParameter=DownloadedColor|NotDownloadedColor}">
												<Path.Data>
													<PathGeometry Figures="M 0.92578125 0 C 0.41455758 1.7184324e-4 1.7184324e-4 0.41455758 0 0.92578125 V 12.302734 c 1.7142932e-4 0.511224 0.41455729 0.92561 0.92578125 0.925782 H 12.302734 c 0.511224 -1.71e-4 0.925611 -0.414558 0.925782 -0.925782 V 0.92578125 C 13.228344 0.41455729 12.813958 1.7142932e-4 12.302734 0 Z" FillRule="NonZero"/>
												</Path.Data>
											</Path>
										</Grid>
									</Button>
									<Button Grid.Row="1" Classes="BlankButton" Height="30" Width="30" Margin="0,5,0,0" Command="{Binding deleteCommand}">
										<Grid>
											<!--delete-->
											<Path IsVisible="{Binding Path=blacklisted, Converter={StaticResource NotConverter}}" Fill="{Binding Path=downloaded, Converter={StaticResource BoolColorConverter}, ConverterParameter=DownloadedColor|NotDownloadedColor}">
												<Path.Data>
